<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jrmf.persistence.CustomNoticeDao">

	<insert id="insertCustomNotice" parameterType="com.jrmf.domain.CustomNotice">
		insert into custom_notice(accountId,noticeId,readIs,enabled,createTime)
		 values(#{accountId},#{noticeId},#{readIs},#{enabled},#{createTime})
	</insert>
</mapper>