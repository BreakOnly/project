<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jrmf.persistence.WechatConfigDao">
	<select id="getWechatConfigByDomainName" resultType="com.jrmf.domain.WechatConfig">
		select * from qb_wechat_config where domainName = #{domainName}
	</select>
	
	<select id="getWechatConfigById" resultType="com.jrmf.domain.WechatConfig">
		select * from qb_wechat_config where id = #{id}
	</select>
	
</mapper>